[run]
omit =
  glue/*tests/*
  glue/qt/ui/*
  glue/core/odict.py,
  glue/core/glue_pickle.py
  glue/external/*
  */site-packages/*

[paths]
source =
    glue/
    */site-packages/glue
    *\site-packages\glue

[report]
omit =
  glue/*tests/*
  glue/qt/ui/*
  glue/core/odict.py,
  glue/core/glue_pickle.py
  glue/external/*
  */site-packages/*
exclude_lines =
  pragma: no cover
  if __name__ == .__main__.:
  raise NotImplementedError
